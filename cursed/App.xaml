<Application x:Class="cursed.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:cursed"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
         <FontFamily x:Key="Karla">Karla ExtraLight</FontFamily>
        <ResourceDictionary x:Key="Style">
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Dictionaries/Dictionary.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
